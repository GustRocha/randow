# -*- coding: utf-8 -*-


def soma(a, b):#toda vez que a def for chamada ela executa a função
	return a+b
s = soma(3, 9)# chamanado a soma 

def mult(a, b):
   return a*b
m = mult(2, 5)
print(soma(s, m))#executa uma função de uma variavel
print(m)
print(s)

import random# importa a função random
numero =  random.randint(0, 10)# variavel pra pegar um numero aleatorio
print (numero)
